{
    "rewrites": [
      { "source": "/products/:match*", "destination": "https://fakestoreapi.com/products/:match*" },
      { "source": "/auth/:match*", "destination": "http://demo1416688.mockable.io/auth/:match*" }
    ],
    "headers": [
      {
        "source": "api/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Credentials", "value": "true" },
          { "key": "Access-Control-Allow-Origin", "value": "*" },
          { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT" }
        ]
      }
    ]
  }